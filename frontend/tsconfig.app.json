/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "types": [
      "gapi",
      "gapi.auth2",
      "dom-mediacapture-record"
    ],
    "typeRoots": [
      "node_modules/@types",
      "./@types"
    ],
    "outDir": "./out-tsc/app",
    "baseUrl": "src",
    "paths": {
      "@env": ["environments/environment"],
      "@shared/*": ["app/shared/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
